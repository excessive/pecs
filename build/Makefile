  config=debug
endif
export config

PROJECTS := pecs

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

pecs: 
	@echo "==== Building pecs ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f pecs.make

clean:
	@${MAKE} --no-print-directory -C . -f pecs.make clean

